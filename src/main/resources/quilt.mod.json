{
	"schema_version": 1,
	"quilt_loader": {
		"group": "com.daimond113",
		"id": "miraculous_miracles",
		"version": "${version}",
		"metadata": {
			"name": "Miraculous Miracles",
			"description": "A mod that adds the Miraculous from the show Miraculous: Tales of Ladybug & Cat Noir.",
			"license": "MIT",
			"contributors": {
				"daimond113": "Owner"
			},
			"contact": {
				"homepage": "https://github.com/daimond113/miraculous-miracles#readme",
				"issues": "https://github.com/daimond113/miraculous-miracles/issues",
				"sources": "https://github.com/daimond113/miraculous-miracles"
			},
			"icon": "assets/miraculous_miracles/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": {
				"adapter": "kotlin",
				"value": "com.daimond113.miraculous_miracles.MiraculousMiracles"
			},
			"client_init": {
				"adapter": "kotlin",
				"value": "com.daimond113.miraculous_miracles.MiraculousMiraclesClient"
			}
		},
		"depends": [
			{
				"id": "qkl",
				"versions": ">=1.0.2-"
			},
			{
				"id": "quilt_loader",
				"versions": ">=0.17.0-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=4.0.0-"
			},
			{
				"id": "minecraft",
				"versions": "1.19.2"
			},
			{
				"id": "patchouli",
				"optional": true
			}
		]
	}
}
